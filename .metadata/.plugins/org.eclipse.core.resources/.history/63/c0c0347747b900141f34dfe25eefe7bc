	 	SortedListOf<String> PrintList = app.getPrintPhoneHelper().getUiPrintPhones();
		
	 	// actions
	 	if dbCotactList.size()==PrintList.size() {
	 		for (int i = 0; i < dbCotactList.size(); i++) {
	 			app.getPrintPhoneHelper().compareName(tempString, i);
